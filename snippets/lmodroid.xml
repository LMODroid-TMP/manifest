<?xml version="1.0" encoding="UTF-8"?>
<manifest>

    <remote name="lmodroid"
        fetch="https://git.libremobileos.com"
        revision="staging/twelve"
        review="gerrit.libremobileos.com" />

    <!-- Build -->
    <project path="build/make" name="LMODroid/platform_build" remote="lmodroid">
        <copyfile src="core/root.mk" dest="Makefile" />
        <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
        <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
        <linkfile src="core" dest="build/core" />
        <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
        <linkfile src="target" dest="build/target" />
        <linkfile src="tools" dest="build/tools" />
    </project>
    <project path="build/soong" name="LMODroid/platform_build_soong" remote="lmodroid" >
        <linkfile src="root.bp" dest="Android.bp" />
        <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
    </project>

    <!-- Hardware -->
    <project path="hardware/google/pixel" name="LMODroid/platform_hardware_google_pixel" remote="lmodroid" />
    <project path="hardware/google/pixel-sepolicy" name="LMODroid/platform_hardware_google_pixel-sepolicy" remote="lmodroid" />
    <project path="hardware/knowles/athletico/sound_trigger_hal" name="LMODroid/platform_hardware_knowles_athletico_sound_trigger_hal" remote="lmodroid" />
    <project path="hardware/qcom/audio" name="LMODroid/platform_hardware_qcom_audio" remote="lmodroid" />
    <project path="hardware/qcom/camera" name="LMODroid/platform_hardware_qcom_camera" remote="lmodroid" />
    <project path="hardware/qcom/data/ipacfg-mgr" name="LMODroid/platform_hardware_qcom_data_ipacfg-mgr" remote="lmodroid" />
    <project path="hardware/qcom/sdm845/display" name="LMODroid/platform_hardware_qcom_sdm845_display" remote="lmodroid" />
    <project path="hardware/qcom/sdm845/media" name="LMODroid/platform_hardware_qcom_sdm845_media" remote="lmodroid" />
    <project path="hardware/qcom/sm7250/display" name="LMODroid/platform_hardware_qcom_sm7250_display" remote="lmodroid" />
    <project path="hardware/qcom/sm7250/media" name="LMODroid/platform_hardware_qcom_sm7250_display" remote="lmodroid" />

    <!-- Manifest -->
    <project path="manifest" name="LMODroid/manifest" remote="lmodroid" />

    <!-- Packages -->
    <project path="packages/resources/devicesettings" name="LMODroid/platform_packages_resources_devicesettings" remote="lmodroid" />

    <!-- Prebuilts -->
    <project path="prebuilts/build-tools" name="LMODroid/platform_prebuilts_build-tools" remote="lmodroid" />
    <project path="prebuilts/tools-lmodroid" name="LMODroid/platform_prebuilts_tools-lmodroid" remote="lmodroid" />

    <!-- Vendor -->
    <project path="vendor/lmodroid" name="LMODroid/platform_vendor_lmodroid" remote="lmodroid" />
    <project path="vendor/support" name="LMODroid/platform_vendor_support" remote="lmodroid" />

</manifest>
